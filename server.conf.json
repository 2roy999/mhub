{
	"listen": {
		"port": 13900
	},
	"verbose": true,
	"nodes": {
		"default": "Exchange",
		"state": {
			"type": "TopicState",
			"options": {
				"persistent": true
			}
		},
		"ping": "PingResponder",
		"blib": {
			"type": "TestSource",
			"options": {
				"topic": "blib",
				"interval": 5000
			}
		},
		"test": {
			"type": "Queue",
			"options": {
				"capacity": 5,
				"pattern": ["test:*", "blib"],
				"persistent": true
			}
		}
	},
	"bindings": [
		{ "from": "blib", "to": "test" }
	],
	"storage": "./storage/"
}
