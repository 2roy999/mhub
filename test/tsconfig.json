{
    "version": "1.7.5",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "isolatedModules": false,
        "declaration": true,
        "noImplicitAny": true,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": true,
        "outDir": "../dist/test/"
    },
    "filesGlob": [
        "../typings/tsd-test.d.ts",
        "./**/*.ts",
        "./**/*.tsx"
    ],
    "files": [
        "../typings/tsd-test.d.ts",
        "./test-index.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
