{
    "src": "{src,typings,test}/**/*.ts",
    "main": "index.html",
    "buildOnSave": true,
    "projectType": "standard",
    "compiler": {
        "noLib": false,
        "removeComments": false,
        "noImplicitAny": true,
        "declaration": true,
        "sourceMap": true,
        "propagateEnumConstants": false,
        "allowAutomaticSemicolonInsertion": false,
        "target": 1,
        "module": 1,
        "outDir": "dist",
        "out": null
    },
    "codeFormat": {
        "NewLineCharacter": "\n",
        "ConvertTabsToSpaces": false,
        "IndentSize": 1,
        "TabSize": 4
    },
    "customBuild": {
        "name": null,
        "command": null,
        "directory": null,
        "environment": null,
        "ownConsole": false
    },
    "customRun": {
        "name": null,
        "command": null,
        "directory": null,
        "environment": null,
        "ownConsole": false
    },
    "documentation": {
        "includeDeclarations": false,
        "outputDirectory": null,
        "readme": null,
        "theme": "default"
    },
    "tslint": {
        "useLint": true,
        "lintFile": "tslint.json"
    }
}