{
    "version": "1.7.5",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "isolatedModules": false,
        "declaration": true,
        "noImplicitAny": true,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": true,
        "outDir": "../dist/src/"
    },
    "filesGlob": [
        "../typings/tsd.d.ts",
        "./**/*.ts",
        "./**/*.tsx"
    ],
    "files": [
        "../typings/tsd.d.ts",
        "./client.ts",
        "./index.ts",
        "./log.ts",
        "./Logger.ts",
        "./MClient.ts",
        "./Message.ts",
        "./ping.ts",
        "./pubsub.ts",
        "./server.ts",
        "./SocketHub.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
