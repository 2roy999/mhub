{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"isolatedModules": false,
		"declaration": true,
		"removeComments": false,
		"noLib": false,
		"preserveConstEnums": true,
		"sourceMap": true,
		"outDir": "../dist/src/",
		"types": [
			"node"
		]
	}
}
